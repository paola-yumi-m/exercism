class RaindropConverter {

    String convert(int number) {
        
    	String result = "";
    	
    	if (number % 3 == 0) {
    		result += "Pling";
    	} 
    	else if (number % 5 == 0) {
    		result += "Plang";
    	}
    	else if (number % 7 == 0) {
    		result += "Plong";
    	}
    	else {
    		result += String.valueOf(number);
    	}
    	
    	return result;
    }

}
